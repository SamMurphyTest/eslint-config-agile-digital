{
  "name": "eslint-config-agile-digital",
  "version": "4.0.0",
  "description": "Agile Digital's standard ESLint config",
  "repository": "https://github.com/agiledigital-labs/eslint-config-agile-digital.git",
  "author": "Daniel Nixon <dan.nixon@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-typed-fp": "^5.0.0 >=5.3.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.0.0 >=2.25.0",
    "eslint-plugin-jest": "^27.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0 >=6.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.0.0 >=7.27.0",
    "eslint-plugin-react-hooks": "^4.0.0 >=4.3.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-total-functions": "^7.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.28",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-agile-digital": "^3.3.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-typed-fp": "^5.3.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.21.0",
    "eslint-plugin-total-functions": "^7.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "ts-jest": "^29.1.1",
    "type-coverage": "^2.27.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives",
    "test-base": "eslint ./test-fixtures/base.ts --report-unused-disable-directives --config index.js --no-eslintrc",
    "test-react": "eslint ./test-fixtures/react.tsx --report-unused-disable-directives --config react.js --no-eslintrc",
    "test-base-react": "eslint ./test-fixtures/base-react.tsx --report-unused-disable-directives --config base-react.js --no-eslintrc",
    "test": "$npm_execpath run test-base && $npm_execpath run test-react && $npm_execpath run test-base-react",
    "release": "yarn build && yarn test && yarn lint && yarn type-coverage && yarn publish"
  },
  "typeCoverage": {
    "atLeast": 100,
    "strict": true,
    "detail": true
  },
  "engines": {
    "node": ">=16.10.0"
  }
}
